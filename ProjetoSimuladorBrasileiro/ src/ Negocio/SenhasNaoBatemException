package NEGOCIOS;
public class SenhasNaoBatemException extends Exception{
	private String senha, confirmarSenha;
	public SenhasNaoBatemException(String senha, String confirmarSenha) {
		super("As senhas n√£o batem.");
		this.senha = senha;
		this.confirmarSenha = confirmarSenha;
	}
	public String getSenha() {
		return senha;
	}
	public String getConfirmarSenha() {
		return confirmarSenha;
	}
	
}
